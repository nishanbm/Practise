<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="211.0" prefWidth="349.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.loginController">
   <children>
      <Label fx:id="lblName" alignment="CENTER" contentDisplay="CENTER" layoutX="86.0" layoutY="9.0" text="MAANSHOVA PHRAMA" textFill="#06bbf2" textOverrun="CENTER_ELLIPSIS">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="58.0" layoutY="41.0" prefHeight="39.0" prefWidth="200.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="58.0" layoutY="86.0" prefHeight="39.0" prefWidth="200.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnLogin" layoutX="153.0" layoutY="158.0" mnemonicParsing="false" onAction="#function" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="244.0" layoutY="158.0" mnemonicParsing="false" onAction="#cancelfunction" text="Cancel">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="lblMessage" layoutX="63.0" layoutY="131.0" prefHeight="17.0" prefWidth="190.0" textFill="#e10707" />
   </children>
</AnchorPane>
